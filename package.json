{
  "name": "@mizdra/eslint-config-mizdra",
  "version": "0.8.0",
  "description": "ESLint config for @mizdra",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./+typescript": "./+typescript.js",
    "./+react": "./+react.js",
    "./+prettier": "./+prettier.js"
  },
  "repository": "https://github.com/mizdra/eslint-config-mizdra",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "check": "run-s -c check:*",
    "check:eslint": "eslint '**/*.js'",
    "check:eslint:fix": "yarn run check:eslint --fix",
    "check:prettier": "prettier --check .",
    "check:prettier:fix": "prettier --write ."
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.11.0",
    "yarn": ">=1.13.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=2.31.0",
    "@typescript-eslint/parser": ">=2.31.0",
    "eslint": ">=6.8.0",
    "eslint-config-prettier": ">=6.11.0",
    "eslint-plugin-import": ">=2.20.2",
    "eslint-plugin-react": ">=7.19.0",
    "eslint-plugin-react-hooks": ">=4.0.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint": {
      "optional": false
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": false
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@mizdra/prettier-config-mizdra": "^0.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.3.2",
    "typescript": "^4.3.2"
  },
  "files": [
    "*.js"
  ]
}
