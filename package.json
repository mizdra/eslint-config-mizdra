{
  "name": "@mizdra/eslint-config-mizdra",
  "version": "6.0.0",
  "description": "ESLint config for @mizdra",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.ts",
        "default": "./src/index.js"
      }
    }
  },
  "repository": "https://github.com/mizdra/eslint-config-mizdra",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --cache --cache-strategy content .",
    "lint:prettier": "prettier --cache --check .",
    "lint:tsc": "tsc",
    "test": "cd test && npm run lint"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": ">=8.33.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-plugin-n": ">=17.10.2",
    "eslint-plugin-react": ">=7.35.0",
    "eslint-plugin-react-hooks": ">=5.2.0",
    "eslint-plugin-simple-import-sort": ">=12.1.1",
    "globals": ">=15.9.0",
    "typescript-eslint": ">=8.5.0"
  },
  "devDependencies": {
    "@mizdra/prettier-config-mizdra": "^2.0.0",
    "@types/eslint__eslintrc": "^3.3.0",
    "@types/eslint__js": "^9.14.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "eslint": "^9.21.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "~3.5.3",
    "typescript": "^5.8.2"
  },
  "files": [
    "src",
    "!src/typings"
  ]
}
