{
  "name": "@mizdra/eslint-config-mizdra",
  "version": "1.2.0",
  "description": "ESLint config for @mizdra",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./+typescript": "./src/+typescript.js",
    "./+react": "./src/+react.js",
    "./+prettier": "./src/+prettier.js"
  },
  "repository": "https://github.com/mizdra/eslint-config-mizdra",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "npm run lint:eslint --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "test": "cd test && npm run lint"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=2.31.0",
    "@typescript-eslint/parser": ">=2.31.0",
    "eslint": ">=6.8.0",
    "eslint-config-prettier": ">=8.0.0",
    "eslint-plugin-import": ">=2.20.2",
    "eslint-plugin-react": ">=7.19.0",
    "eslint-plugin-react-hooks": ">=4.0.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint": {
      "optional": false
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": false
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.0.2",
    "@eslint/js": "^8.38.0",
    "@mizdra/prettier-config-mizdra": "^1.0.0",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.8.7",
    "typescript": "^5.0.3"
  },
  "files": [
    "*.js"
  ]
}
