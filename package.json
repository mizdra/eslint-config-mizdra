{
  "name": "@mizdra/eslint-config-mizdra",
  "version": "7.0.0",
  "description": "ESLint config for @mizdra",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.d.ts",
        "default": "./src/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mizdra/eslint-config-mizdra.git"
  },
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --cache --cache-strategy content .",
    "lint:prettier": "prettier --cache --check .",
    "lint:tsc": "tsc",
    "test": "cd test && npm run lint"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "engines": {
    "node": ">=20.19.0"
  },
  "peerDependencies": {
    "eslint": ">=9.10.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": ">=9.39.2",
    "eslint-config-prettier": ">=10.1.8",
    "eslint-plugin-n": ">=17.23.2",
    "eslint-plugin-react": ">=7.37.5",
    "eslint-plugin-react-hooks": ">=7.0.1",
    "eslint-plugin-simple-import-sort": ">=12.1.1",
    "globals": ">=17.0.0",
    "typescript-eslint": ">=8.53.0"
  },
  "devDependencies": {
    "@mizdra/prettier-config-mizdra": "^2.0.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "eslint": "^9.39.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "~3.8.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "src",
    "!src/typings"
  ]
}
