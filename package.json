{
  "name": "@mizdra/eslint-config-mizdra",
  "version": "4.0.0",
  "description": "ESLint config for @mizdra",
  "exports": {
    ".": "./src/index.js",
    "./+node": "./src/+node.js",
    "./+typescript": "./src/+typescript.js",
    "./+react": "./src/+react.js",
    "./+prettier": "./src/+prettier.js",
    "./flat": {
      "import": {
        "types": "./src/flat/index.d.mts",
        "default": "./src/flat/index.mjs"
      }
    }
  },
  "repository": "https://github.com/mizdra/eslint-config-mizdra",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --cache --cache-strategy content .",
    "lint:prettier": "prettier --cache --check .",
    "lint:tsc": "tsc",
    "test": "cd test && npm run lint"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@types/eslint": ">=8.56.11",
    "eslint": ">=8.50.0",
    "prettier": ">=2.0.5",
    "typescript": ">=3.9.3"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "eslint": {
      "optional": false
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": ">=8.33.0",
    "@typescript-eslint/eslint-plugin": ">=8.1.0",
    "@typescript-eslint/parser": ">=8.1.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-plugin-import-x": ">=3.1.0",
    "eslint-plugin-n": ">=17.10.2",
    "eslint-plugin-react": ">=7.35.0",
    "eslint-plugin-react-hooks": ">=4.6.2",
    "globals": ">=15.9.0",
    "typescript-eslint": ">=8.1.0"
  },
  "devDependencies": {
    "@mizdra/prettier-config-mizdra": "^2.0.0",
    "@types/eslint": "^9.6.0",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "eslint": "^9.9.1",
    "npm-run-all2": "^6.2.2",
    "prettier": "~3.3.3",
    "typescript": "^5.5.4"
  },
  "files": [
    "src",
    "!src/typings"
  ]
}
